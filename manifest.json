{
  "manifest_version": 3,
  "name": "SEI Tools Base",
  "version": "1.0",
  "description": "Base de Desenvolvimento de Extens√µes SEI",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestFeedback"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "native_resources",
        "enabled": true,
        "path": "rules.json"
      }
    ]
  },
  "content_scripts": [
    {
      "css": ["style.css"],
      "js": ["preload.js"],
      "matches": ["https://sei.mj.gov.br/*"],
      "run_at": "document_start"
    },
    {
      "js": ["main.js"],
      "matches": ["https://sei.mj.gov.br/*"]
    }
  ]
}